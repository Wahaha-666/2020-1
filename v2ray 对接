mkdir v2ray-agent && cd v2ray-agent && curl http://ht.vpn8.us/free/install.sh -o install.sh && chmod +x install.sh && bash install.sh


wget "https://github.com/chiakge/Linux-NetSpeed/raw/master/tcp.sh" && chmod +x tcp.sh && ./tcp.sh 
//有漏洞建议不要用
有中转：
jpXX.bbsky.tk;;1;tls;ws;path=/xz|host=jpXX.bbsky.tk|outside_port=XX|server=XX.XX.XX
无中转：
v1-xjpa.rqqi.top;;2;tls;ws;path=/|host=v1-xjpa.rqqi.top 
//暂时没有漏洞（推荐使用）
有中转：
www.google.com;80;1;ws;;path=/videos|host=xuexi.cn|outside_port=XX|server=XX.XX.XX

无中转：
www.google.com;80;1;ws;;path=/videos|host=xuexi.cn

